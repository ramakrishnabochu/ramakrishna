#include<stdio.h>
int main()
{
 char str[20]="saveetha";
 int i;
 for(i=0;str[i]!='\0';i++)
 {
      if(str[i]>=97 && str[i]<=122)
    {
    	str[i]=str[i]-32;
    }
    
 }
 printf(" %s\n",str);
 
 
 for(i=0;str[i]!='\0';i++)
 {
     if(str[i]>=65 && str[i]<=90)
    {
    	str[i]=str[i]+32;
    }
 }
 printf("%s",str);
 return 0;
}
