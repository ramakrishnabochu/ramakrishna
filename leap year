#include<stdio.h>
int main()
{
 int year=2024,month=3,date=2;
 if (year % 400 == 0) {
 printf("%d/%d/%d is a leap year.", date,month,year);
 }
 else if (year % 100 == 0) {
 printf("%d/%d/%d is not a leap year.",date,month, year);
 }
 else if (year % 4 == 0) {
 printf("%d/%d/%d is a leap year.", date,month,year);
 }
 else {
 printf("%d/%d/%d is not a leap year.", date,month,year);
 }
 return 0;
}
