#include<stdio.h>
int main()
{
int odd_sum=0,even_sum=0,o_count=0,e_count=0,i;
for(i=1;i<=10;i++)
{
if(i%2==0)
{
even_sum=even_sum+i;
e_count++;
}
else
{
odd_sum=odd_sum+i;
o_count++;
}
}
printf("the even_sum=%d\n",even_sum);
printf("the odd_sum=%d\n",odd_sum);
printf("the even count=%d\n",e_count);
printf("the odd count =%d",o_count);
return 0;
}
